{
  "mcpServers": {
    "github.com/awslabs/mcp/tree/main/src/terraform-mcp-server": {
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "args": [
        "awslabs.terraform-mcp-server@latest"
      ],
      "autoApprove": [
        "SearchUserProvidedModule",
        "SearchSpecificAwsIaModules",
        "SearchAwsccProviderDocs",
        "SearchAwsProviderDocs"
      ],
      "disabled": false,
      "command": "uvx",
      "disabled_resources": []
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/filesystem": {
      "env": [],
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/yss/PROJECTS/"
      ],
      "disabled": false,
      "command": "/Users/yss/.local/share/mise/shims/npx",
      "autoApprove": [
        "read_file",
        "read_multiple_files",
        "list_directory",
        "search_files",
        "get_file_info",
        "list_allowed_directories",
        "directory_tree",
        "move_file",
        "write_file",
        "create_directory",
        "edit_file"
      ]
    },
    "github.com/tavily-ai/tavily-mcp": {
      "env": {
        "TAVILY_API_KEY": "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"
      },
      "args": [
        "-y",
        "tavily-mcp@0.2.1"
      ],
      "disabled": false,
      "command": "/Users/yss/.local/share/mise/shims/npx",
      "autoApprove": [
        "tavily-search",
        "tavily-extract",
        "tavily-map",
        "tavily-crawl"
      ]
    },
    "github.com/awslabs/mcp/tree/main/src/aws-documentation-mcp-server": {
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "disabled": false,
      "command": "uvx",
      "autoApprove": [
        "read_documentation",
        "search_documentation",
        "recommend"
      ]
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/sequentialthinking": {
      "disabled": false,
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "/Users/yss/.local/share/mise/shims/npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled_prompts": [
        "create_native_server"
      ],
      "disabled_resources": [
        "mcphub://docs",
        "mcphub://native_server_guide",
        "mcphub://changelog"
      ]
    },
    "neovim": {
      "disabled_prompts": [
        "parrot"
      ],
      "disabled_resources": [
        "neovim://diagnostics/current",
        "neovim://diagnostics/workspace",
        "neovim://buffer"
      ]
    }
  }
}

