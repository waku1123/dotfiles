{
  "link": {
    "prefix": ".link",
    "body": [
      "[${1:link text}](${2:link url})"
    ],
    "description": "Markdown Link"
  },
  "image": {
    "prefix": ".image",
    "body": [
      "![${1:image text}](${2:image url})"
    ],
    "description": "Markdown Image"
  },
  "table-header": {
    "prefix": ".t-header",
    "body": [
      "| ${1:col name1} | ${2:col name2} | ${3:col name3} |",
      "| --- | --- | --- |"
    ],
    "description": "Table Header"
  },
  "table-row": {
    "prefix": ".t-row",
    "body": [
      "| ${1:row name1} | ${2:row name2} | ${3:row name3} |"
    ],
    "description": "Table Row"
  }
  "details": {
    "prefix": ".details",
    "body": [
      "<details>",
      "<summary>${1:title}</summary>",
      "",
      "$2",
      "",
      "</details>"
    ],
    "description": "Summary"
  },
  "warning-call-out": {
    "prefix": ".warning",
    "body": [
      "> [!WARNING]",
      "> ",
      "> ${1:content}"
    ],
    "description": "Call-out Warning"
  },
  "note-call-out": {
    "prefix": ".note",
    "body": [
      "> [!NOTE]",
      "> ",
      "> ${1:content}"
    ],
    "description": "Call-out Note"
  }
}
